{
    "name": "lemaur/cms",
    "description": "cms",
    "keywords": [
        "lemaur",
        "cms"
    ],
    "homepage": "https://github.com/lemaur/cms",
    "license": "MIT",
    "authors": [
        {
            "name": "Maurizio",
            "email": "hello@lemaur.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "artesaos/seotools": "^v0.20.1",
        "illuminate/contracts": "^8.0|^9.0",
        "lemaur/eloquent-publishing": "^1.0",
        "lemaur/laravel-image-sitemap": "^1.0",
        "lemaur/markdown": "^0.2.0",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-medialibrary": "^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.4",
        "spatie/laravel-schemaless-attributes": "^2.1",
        "spatie/laravel-sluggable": "^3.0",
        "spatie/laravel-tags": "^4.0",
        "spatie/laravel-view-models": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.13|^7.0",
        "phpunit/phpunit": "^9.3",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ray": "^1.9",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Lemaur\\Cms\\": "src",
            "Lemaur\\Cms\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Lemaur\\Cms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "format" : "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lemaur\\Cms\\CmsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
