{
    "name": "lemaur/laravel-cms",
    "description": "laravel-cms",
    "keywords": [
        "lemaur",
        "laravel-cms"
    ],
    "homepage": "https://github.com/lemaur/laravel-cms",
    "license": "MIT",
    "authors": [
        {
            "name": "Maurizio",
            "email": "hello@lemaur.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0",
        "lemaur/eloquent-publishing": "^1.0",
        "spatie/eloquent-sortable": "^3.11",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "spatie/laravel-schemaless-attributes": "^1.8",
        "spatie/laravel-sluggable": "^2.6",
        "spatie/laravel-tags": "^4.0",
        "spatie/laravel-view-models": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.13",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Lemaur\\Cms\\": "src",
            "Lemaur\\Cms\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lemaur\\Cms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "format" : "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lemaur\\Cms\\CmsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
